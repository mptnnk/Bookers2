<!--bookers2 users#index ユーザーの一覧-->
<%= render 'userinfo',user:@user %>
<%= render 'books/booknew',book:@book %>

<div class="indexarea">
 <h1>Users</h1>
 <% @users.each do |user|%>
  <%= image_tag user.get_image %>
  <p>name <%= user.name %></p>
  <%= link_to "Show",user_path(user.id)%>
 <% end %>
</div>